{
  "manifest_version": 2,
  "name": "Page Scraper",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_security_policy": "script-src 'self' https://chrome-scraping-extension.glitch.me; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "frog_guiro_128.jpg"
  }
}